<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.crud.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button id="btnLogin" fx:id="btnLogin" layoutX="327.0" layoutY="204.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <TextField id="tfUsername" fx:id="txtusername" layoutX="224.0" layoutY="116.0" />
      <TextField id="tfPass" fx:id="txtpassword" layoutX="224.0" layoutY="166.0" />
      <Text layoutX="153.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username: " />
      <Text layoutX="156.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <Text layoutX="153.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an account?">
         <font>
            <Font size="10.0" />
         </font>
      </Text>
      <Button fx:id="btnSignUp" layoutX="153.0" layoutY="283.0" mnemonicParsing="false" onAction="#signup" text="Sign Up" />
   </children>

</AnchorPane>
